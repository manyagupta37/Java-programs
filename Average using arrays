int n;
        
        float sum=0;
        Scanner sc=new Scanner(System.in);
        
        n=sc.nextInt();

        float[]arr=new float[n];
        for(int i=0;i<n;i++)
        {
            arr[i]=sc.nextFloat();
        }


        for(int i=0;i<n;i++)
        {
            sum+=arr[i];
        }

        float avg=sum/n;

        System.out.printf("%.2f",avg);

    }
}
