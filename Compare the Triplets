public static List<Integer> compareTriplets(List<Integer> a, List<Integer> b) {
        List<Integer>l=new ArrayList<>(Arrays.asList(0,0));
        
for(int i=0;i<a.size();i++)
{
    
        if(a.get(i)>b.get(i))
        {
            l.set(0,l.get(0)+1);
            
        }
        else if(a.get(i)<b.get(i))
        {
            l.set(1,l.get(1)+1);
            
        }
        
    }

return l;
    }
