
/* Structure of node
class Node
{
    int data;
    Node next;
    Node(int d) {data = d; next = null; }
} */

class Solution {

    // Function to find the data of kth node from
    // the end of a linked list.
    int getKthFromLast(Node head, int k) {
        // Your code here
        if(head==null)
        {
            return 0;
        }
        Node slow=head;
        Node fast=head;
        for(int i=1;i<=k;i++)
        {
            fast=fast.next;
        }
        while(fast!=null){
        slow=slow.next;
        fast=fast.next;
        }
        return slow.data;
        
    }
}
