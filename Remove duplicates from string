class Solution {
    String removeDuplicates(String s) {
        LinkedHashSet<Character> set=new LinkedHashSet<>();
        StringBuilder sb=new StringBuilder();
        for(int i=0;i<s.length();i++)
        {
            set.add(s.charAt(i));
        }
        for(char ch:set)
        {
            sb.append(ch);
        }
        return sb.toString();
    }
}
