class Solution {

    static ArrayList<Integer> removeDuplicate(int arr[]) {
        // code here
        
        Set<Integer>set=new LinkedHashSet<>();
        ArrayList<Integer>res=new ArrayList<>();
        for(int i=0;i<arr.length;i++)
        {
            set.add(arr[i]);
        }
        
        for(int num:set)
        {
            res.add(num);
        }
        return res;
        
        
    }
}
