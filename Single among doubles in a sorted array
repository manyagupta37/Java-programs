class Solution {
    int findOnce(int[] arr) {
        // Complete this function
        if(arr.length<2)
        {
            return arr[0];
        }
        for(int i=0;i<arr.length-1;i+=2)
        {
            if(arr[i]!=arr[i+1])
            {
                return arr[i];
            }
        }
        return arr[arr.length-1];
        
    }
}
